{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "ts-node",
        "type": "node",
        "request": "launch",
        "args": ["${relativeFile}"],
        "runtimeArgs": ["--require", "ts-node/register", "--require", "./instrumentation.ts"],
        "cwd": "${workspaceRoot}",
        "internalConsoleOptions": "openOnSessionStart"
      }
    ]
  }